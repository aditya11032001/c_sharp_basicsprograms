using System;

namespace array_elementSUM_using_loop
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] numbers = { 10, 20, 30, 40, 50 };
            int sum = 0;

            for(int i=0; i <numbers.Length;i++)
            {
                sum += numbers[i];
            }
            Console.WriteLine($"The sum of the array elements is: {sum}");
            Console.ReadKey();
        }
    }
}
